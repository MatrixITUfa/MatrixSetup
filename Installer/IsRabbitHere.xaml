      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Installer"
      mc:Ignorable="d"
      Title="IsRabbitHere" Background="White" Height="450" Width="800">

    <Grid HorizontalAlignment="Left" Width="670" Height="186" VerticalAlignment="Top" Margin="66,54,0,0">
