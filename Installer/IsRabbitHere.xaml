        <Button x:Name="btn_Have_Rabbit" Content="Да" HorizontalAlignment="Left" Margin="76,100,0,0" VerticalAlignment="Top" Height="75" Width="149" FontSize="36" FontWeight="Normal"/>
        <Button x:Name="btn_No_Rabbit" Content="Нет" HorizontalAlignment="Left" Margin="420,100,0,0" VerticalAlignment="Top" Height="75" Width="149" FontSize="36"/>
        <Grid/>

    </Grid>
